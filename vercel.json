{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "backend/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/backend/src/index.js"
        }
    ]
}